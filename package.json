  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "confd": "node ./confd/generate-config.js",
    "confd:prod": "node ./confd/generate-config.js --environment production"
  },
  "dependencies": {
    "@map-colonies/mc-logger": "^1.2.1",
    "@map-colonies/mc-probe": "^1.0.0",
    "@map-colonies/nest-schema-validator": "^1.0.2",
    "@nestjs/common": "^7.4.2",
    "@nestjs/core": "^7.4.2",
    "@nestjs/platform-express": "^7.4.2",
    "@nestjs/swagger": "^4.7.16",
    "@nestjs/terminus": "^7.0.1",
    "aws-sdk": "^2.734.0",
    "axios": "^0.19.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "config": "^3.3.1",
    "file-uri-to-path": "^2.0.0",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.20",
    "multer-s3": "^2.9.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "swagger-ui-express": "^4.1.4",
    "upath": "^1.2.0"
  },
  "devDependencies": {
    "@map-colonies/mc-model-types": "^1.0.1",
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.4.2",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.10",
    "@types/multer": "^1.4.4",
    "@types/multer-s3": "^2.7.7",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
